{
  "alignTernaryLines": false,
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "generatorStarSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "indentChains": true,
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 92,
  "proseWrap": "preserve",
  "requirePragma": false,
  "semi": false,
  "singleQuote": true,
  "spaceAfterFunction": true,
  "spaceBeforeFunctionParen": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "yieldStarSpacing": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": ".babelrc",
      "options": { "parser": "json" }
    }
  ]
}
