{
  "parserOptions": {
    "ecmaVersion": 2018,
    "impliedStrict": true,
    "sourceType": "module"
  },
  "env": {
    "es2017": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".mjs",
          ".web.js",
          ".ios.js",
          ".android.js",
          ".jsx"
        ]
      }
    },
    "react": {
      "version": "16.13.1"
    },
    "import/extensions": [
      ".js",
      ".mjs",
      ".web.js",
      ".ios.js",
      ".android.js",
      ".jsx"
    ]
  },
  "plugins": [
    "import",
    "node",
    "promise",
    "standard",
    "unicorn",
    "react",
    "react-hooks"
  ],
  "extends": [
    "standard"
  ],
  "rules": {
    "comma-dangle": [
      2,
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ],
    "complexity": [
      1,
      {
        "max": 10
      }
    ],
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "max-depth": 1,
    "no-case-declarations": 1,
    "no-misleading-character-class": 2,
    "spaced-comment": [
      2,
      "always",
      {
        "block": {
          "exceptions": [
            "*"
          ],
          "markers": [
            "*package",
            "!",
            ",",
            ":",
            "::",
            "flow-include",
            "#__PURE__"
          ],
          "balanced": true
        },
        "line": {
          "exceptions": [
            "-",
            "="
          ],
          "markers": [
            "*package",
            "!",
            "/",
            ",",
            "=",
            "#region",
            "#endregion",
            "#"
          ]
        }
      }
    ],
    "wrap-iife": [
      2,
      "inside"
    ],
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "default-case": [
      1,
      {
        "commentPattern": "^no ?default$"
      }
    ],
    "default-param-last": 2,
    "for-direction": 2,
    "no-constructor-return": 2,
    "max-len": [
      2,
      {
        "code": 92,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "getter-return": 2,
    "no-alert": 2,
    "no-async-promise-executor": 2,
    "no-await-in-loop": 2,
    "no-confusing-arrow": [
      2,
      {
        "allowParens": false
      }
    ],
    "no-div-regex": 2,
    "no-dupe-else-if": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-extra-label": 2,
    "no-extra-semi": 2,
    "no-global-assign": 2,
    "no-implicit-globals": 2,
    "no-import-assign": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-prototype-builtins": 2,
    "no-restricted-globals": [
      "error",
      "title",
      "status",
      "self",
      "addEventListener",
      "blur",
      "close",
      "closed",
      "confirm",
      "defaultStatus",
      "defaultstatus",
      "event",
      "external",
      "find",
      "focus",
      "frameElement",
      "frames",
      "history",
      "innerHeight",
      "innerWidth",
      "length",
      "location",
      "locationbar",
      "menubar",
      "moveBy",
      "moveTo",
      "name",
      "onblur",
      "onerror",
      "onfocus",
      "onload",
      "onresize",
      "onunload",
      "open",
      "opener",
      "opera",
      "outerHeight",
      "outerWidth",
      "pageXOffset",
      "pageYOffset",
      "parent",
      "print",
      "removeEventListener",
      "resizeBy",
      "resizeTo",
      "screen",
      "screenLeft",
      "screenTop",
      "screenX",
      "screenY",
      "scroll",
      "scrollbars",
      "scrollBy",
      "scrollTo",
      "scrollX",
      "scrollY",
      "statusbar",
      "stop",
      "toolbar",
      "top"
    ],
    "no-restricted-syntax": [
      2,
      "WithStatement"
    ],
    "no-script-url": 2,
    "no-setter-return": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-unused-labels": 2,
    "no-useless-concat": 2,
    "one-var-declaration-per-line": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "prefer-const": [
      2,
      {
        "destructuring": "all"
      }
    ],
    "prefer-numeric-literals": 2,
    "radix": 2,
    "require-atomic-updates": 2,
    "require-await": 2,
    "require-yield": 2,
    "sort-imports": [
      1,
      {
        "ignoreCase": true,
        "ignoreDeclarationSort": true,
        "ignoreMemberSort": false
      }
    ],
    "switch-colon-spacing": [
      2,
      {
        "after": true
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "prefer-arrow-callback": 0,
    "quote-props": [
      2,
      "consistent"
    ],
    "no-useless-constructor": 0,
    "promise/catch-or-return": [
      2,
      {
        "allowThen": true,
        "terminationMethod": [
          "catch",
          "finally"
        ]
      }
    ],
    "promise/no-callback-in-promise": 2,
    "promise/no-nesting": 1,
    "promise/no-new-statics": 2,
    "promise/no-return-in-finally": 2,
    "promise/no-return-wrap": [
      2,
      {
        "allowReject": true
      }
    ],
    "promise/valid-params": 2,
    "import/default": 2,
    "import/export": 2,
    "import/first": 2,
    "import/named": 2,
    "import/namespace": 2,
    "import/no-absolute-path": 2,
    "import/no-cycle": [
      2,
      {
        "maxDepth": 3,
        "ignoreExternal": true
      }
    ],
    "import/no-deprecated": 1,
    "import/no-dynamic-require": 2,
    "import/no-mutable-exports": 2,
    "import/no-named-as-default-member": 2,
    "import/no-named-as-default": 2,
    "import/no-named-default": 2,
    "import/no-self-import": 2,
    "import/no-unresolved": [
      2,
      {
        "commonjs": true
      }
    ],
    "import/no-useless-path-segments": 1,
    "import/no-webpack-loader-syntax": 2,
    "import/order": [
      1,
      {
        "groups": [
          "builtin",
          "external"
        ]
      }
    ],
    "unicorn/custom-error-definition": 2,
    "unicorn/error-message": 2,
    "unicorn/new-for-builtins": 2,
    "unicorn/no-array-instanceof": 2,
    "unicorn/no-console-spaces": 1,
    "unicorn/no-new-buffer": 2,
    "unicorn/no-process-exit": 2,
    "unicorn/no-unreadable-array-destructuring": 2,
    "unicorn/no-unused-properties": 1,
    "unicorn/prefer-add-event-listener": 1,
    "unicorn/prefer-dataset": 1,
    "unicorn/prefer-event-key": 2,
    "unicorn/prefer-includes": 2,
    "unicorn/prefer-modern-dom-apis": 1,
    "unicorn/prefer-negative-index": 2,
    "unicorn/prefer-node-remove": 1,
    "unicorn/prefer-spread": 2,
    "unicorn/prefer-starts-ends-with": 1,
    "unicorn/prefer-text-content": 2,
    "unicorn/prefer-type-error": 2,
    "unicorn/better-regex": 2,
    "unicorn/throw-new-error": 2,
    "node/no-unpublished-bin": 2,
    "node/no-unsupported-features/node-builtins": 2,
    "node/no-mixed-requires": 2,
    "node/no-deprecated-api": 2,
    "node/no-process-exit": 2
  },
  "globals": {
    "__DEV__": "readonly"
  }
}